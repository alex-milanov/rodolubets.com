{
  "name": "rodolubets.com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:assets && npm run build:js && npm run build:sass",
    "build:js": "browserify src/js/index.js -o dist/js/app.js -t [ babelify --presets [ es2015 ] ]",
    "build:sass": "node-sass --source-map dist/css/style.css.map --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/css/style.css",
    "build:assets": "node bin/move-assets.js",
    "watch": "npm run watch:js & npm run watch:sass & npm run watch:assets & npm run livereload",
    "watch:js": "watchify -p browserify-hmr src/js/index.js -o dist/js/app.js --debug",
    "watch:sass": "nodemon -w src/sass -e sass -x 'npm run build:sass'",
    "watch:assets": "nodemon -e jpg,jpeg,gif,png,json,md --watch src/assets --exec 'npm run build:assets'",
    "livereload": "livereload dist/**/*.css -d",
    "start": "npm run watch & nodemon server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "iblokz-data": "^1.1.0",
    "iblokz-rest-api": "^1.3.0",
    "iblokz-snabbdom-helpers": "^1.2.0",
    "jsonwebtoken": "^7.3.0",
    "marked": "^0.3.6",
    "method-override": "^2.3.6",
    "moment": "^2.15.1",
    "mongoose": "^4.6.3",
    "multer": "^1.3.0",
    "rx": "^4.1.0",
    "snabbdom": "^0.5.3",
    "superagent": "^2.3.0",
    "to-markdown": "^3.0.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.5",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^0.30.0",
    "livereload": "^0.5.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "watchify": "^3.9.0"
  }
}
